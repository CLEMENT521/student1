pipeline {
    agent any

    environment {
        SONARQUBE_HOME = tool 'SonarQube'
    }
 stages {
   stage('Quality Gate') {
            steps {
                script {
                    // Execute SonarQube scanner
                    withSonarQubeEnv('SonarQubeServer') {
                        sh "${SONARQUBE_SCANNER_HOME}/bin/sonar-scanner \
                            -Dsonar.projectKey=test2 \
                            -Dsonar.sources=./"
                    }
            }
        }
     }
}
